{
  "name": "tlk.io web",
  "description": "Chat on the pages of the websites you visit",
  "version": "0.0.1",
  "author": "mustafauzun0",
  "manifest_version": 2,
  "browser_action":{
    "default_icon": "icon.png",
  	"default_popup": "index.html",
  	"default_title": "tlk.io web",
    "content_scripts": [
    {
        "css": ["css/index.css"],
        "js": ["js/create.js"]
    }]
  },
  "permissions": [
    "tabs"
],
  "icons": {
    "128": "icon.png",
    "256": "icon-256.png"
  }
}